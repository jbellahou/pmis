@{
    Layout = "_Layout.cshtml";
}

<div class="container">
    <div class="pmis-detail-page-heading">
        <h3 class="pmis-detail-page-title">Add New Administrator</h3>
    </div>
    <form method="post" action="administrators?id=@Request.QueryString["id"]" data-parsley-validate>
        <div class="form-group row">
            <label for="aemail">Email Address</label>
            <input type="email" class="form-control" name="aemail" id="aemail" autofocus required data-parsley-required data-parsley-type="email">
        </div>
        <div class="form-group row">
            <button class="btn btn-primary" type="submit" name="SaveType" value="Add" title="Save"><i class="fas fa-save"></i></button>
        </div>
    </form>
</div>